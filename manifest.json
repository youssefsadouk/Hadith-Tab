{
    "name": "Hadith Tab",
    "version": "1.0.1",
    "manifest_version" : 3,
    "description": "A Chrome Extension that shows Prophet Muhamed's Hadith once a tab is opened",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "128": "icons/quran.png"
    },
    "chrome_url_overrides": {
        "newtab": "popup.html"
    },
    "permissions": ["activeTab"]
}